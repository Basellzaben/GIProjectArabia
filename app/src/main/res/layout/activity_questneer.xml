                android:layout_marginRight="10dp"
                android:onClick="btn_back" />

            <ImageButton
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:id="@+id/imageButton9"
                android:src="@drawable/share"
                android:onClick="btn_share" />

